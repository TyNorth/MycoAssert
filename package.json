{
  "name": "@simdev01/mycoassert",
  "private": false,
  "version": "1.0.0",
  "description": "A lightweight code generator that creates zero-dependency, runtime validation functions and TypeScript types from a JSON schema.",
  "main": "generate-validators.cjs",
  "bin": {
    "mycoassert": "generate-validators.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "generate": "node ./generate-validators.cjs",
    "generate:dts": "node ./generate-validators.cjs --dts",
    "watch": "node ./generate-validators.cjs --watch"
  },
  "keywords": [
    "validation",
    "codegen",
    "code-generator",
    "schema",
    "json",
    "type-safety",
    "runtime",
    "typescript",
    "d.ts"
  ],
  "author": "Ty North <north.tyjr@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TyNorth/mycoassert.git"
  },
  "bugs": {
    "url": "https://github.com/TyNorth/mycoassert/issues"
  },
  "homepage": "https://github.com/TyNorth/mycoassert#readme",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/eslint-plugin": "^1.1.39",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "chokidar": "^4.0.3",
    "eslint": "^9.22.0",
    "eslint-plugin-oxlint": "^0.16.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.0",
    "prettier": "3.5.3",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.2.3",
    "vue": "^3.5.13",
    "yargs": "^18.0.0"
  }
}
